{
  "name": "pokeranking",
  "private": true,
  "engines": {
    "node": "24.x",
    "npm": "11.x"
  },
  "workspaces": [
    "backend",
    "frontend",
    "packages/*"
  ],
  "scripts": {
    "api:export": "npm run export:openapi -w backend",
    "api:generate": "npm run generate -w @pokeranking/api-client",
    "api:build": "npm run build -w @pokeranking/api-client",
    "api:full": "npm run api:export && npm run api:build"
  }
}