{
  "name": "@pokeranking/shared",
  "version": "0.1.0",
  "description": "Shared constants and utilities for Pokemon Ranking",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "rimraf": "^6.0.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0"
  },
  "keywords": [
    "pokemon",
    "ranking",
    "shared",
    "constants"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/YOUR_USERNAME/pokeranking.git",
    "directory": "packages/shared"
  },
  "license": "MIT"
}
