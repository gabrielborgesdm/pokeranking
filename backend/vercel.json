{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "functions": {
        "dist/**/*.js": {
            "includeFiles": "dist/src/i18n/**"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/src/main.js",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE",
                "PATCH",
                "OPTIONS"
            ]
        }
    ]
}